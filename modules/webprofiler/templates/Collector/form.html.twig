{% block toolbar %}
    {% set icon %}
    <a href="{{ url("webprofiler.profiler", {token: token}, {fragment: 'form'}) }}">
        <img width="21" height="28" alt="Form"
             src="data:image/png;base64,">
        <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.forms|length }}</span>
    </a>
    {% endset %}
    {% set text %}
    {% spaceless %}
        <div class="sf-toolbar-info-piece">
            <b>{{ 'Forms'|t }}</b>
            <span>{{ collector.forms|length }}</span>
        </div>
    {% endspaceless %}
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
        <div class="sf-toolbar-info">{{ text|default('') }}</div>
    </div>
{% endblock %}

{% block panel %}
    <div class="title">
        <h2>{{ 'Forms'|t }}</h2>
    </div>

    <div class="content">
        {{ content }}
    </div>
{% endblock %}
