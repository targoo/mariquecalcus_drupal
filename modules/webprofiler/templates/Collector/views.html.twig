{% block toolbar %}
    {% set icon %}
    <a href="{{ url("webprofiler.profiler", {token: token}, {fragment: 'views'}) }}">
        <img width="20" height="28" alt="Asset"
             src="data:image/png;base64,"/>
        <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.viewsCount }}</span>
    </a>
    {% endset %}
    {% set text %}

    <div class="sf-toolbar-info-piece">
        <b>{{ 'Views'|t  }}</b>
        <span>{{ collector.viewsCount }}</span>
    </div>
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
        <div class="sf-toolbar-info">{{ text|default('') }}</div>
    </div>
{% endblock %}

{% block panel %}
    <div class="title">
        <h2>{{ 'Views'|t }}</h2>
    </div>

    <div class="content">
        {{ content }}
    </div>
{% endblock %}
