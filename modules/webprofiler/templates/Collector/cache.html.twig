{% block toolbar %}
    {% set icon %}
    <a href="{{ url("webprofiler.profiler", {token: token}, {fragment: 'cache'}) }}">
        <img width="20" height="28" alt="Cache"
             src="data:image/png;base64,"/>
        <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.countcacheHits }} / {{ collector.countcacheMisses }}</span>
    </a>
    {% endset %}
    {% set text %}

    {% for bin, cids in collector.cacheHits %}
        <div class="sf-toolbar-info-piece">
            <b>{{ bin }}</b>
            <span>{{ cids|length }}</span>
        </div>
    {% endfor %}
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
        <div class="sf-toolbar-info">{{ text|default('') }}</div>
    </div>
{% endblock %}

{% block panel %}
    <div class="title">
        <h2>{{ 'Cache'|t }}</h2>
    </div>

    <div class="content">
        {{ content }}
    </div>
{% endblock %}
